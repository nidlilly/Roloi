<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <io.esper.roloi.JCalendarMonthTopView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/jcalendarmonthview"
        app:dayHeight="32sp"
        app:daytextsize="11sp"
        app:datetextsize="12.4sp"
        app:eventtextsize="10sp"
        app:daytextcolor="#787878"
        app:datetextcolor="#050505"
        app:datemargintop="0sp"
        android:background="@android:color/white"
        app:linewidth="1.1sp"
        app:linecolor="#EDEDED"
        />


</RelativeLayout>